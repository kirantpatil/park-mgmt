<p id="notice"><%= notice %></p>

<h1>Listing Ccunits</h1>

<table>
  <thead>
    <tr>
      <th>Ip</th>
      <th>Port</th>
      <th>Floor</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ccunits.each do |ccunit| %>
      <tr>
        <td><%= ccunit.ip %></td>
        <td><%= ccunit.port %></td>
        <td><%= ccunit.floor.name %></td>
        <% if current_user_admin? %>
        <td><%= link_to 'Show', ccunit %></td>
        <td><%= link_to 'Edit', edit_ccunit_path(ccunit) %></td>
        <td><%= link_to 'Destroy', ccunit, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        <% end %>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<% if current_user_admin? %>
<%= link_to 'New Ccunit', new_ccunit_path %>
<% end %>
